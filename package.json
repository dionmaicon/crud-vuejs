{
  "name": "vue-crudgen",
  "version": "1.0.6",
  "description": "TryUs. Software CRUD (Create, Read, Update and Delete) basic generator for Vue.js",
  "author": "Dion Maicon E. Duarte <dionmaicon@outlook.com>",
  "scripts": {
    "dev": "npm run serve",
    "test": "jest",
    "lint-staged": "lint-staged",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add",
      "jest --bail --findRelatedTests"
    ]
  },
  "bin": {
    "vue-crudgen": "./main.js"
  },
  "preferGlobal": true,
  "dependencies": {
    "commander": "^4.0.0",
    "pluralize": "^8.0.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.9",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.0.1",
    "faker": "^4.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.0"
  },
  "bugs": {
    "url": "https://github.com/dionmaicon/vue-crudgen/issues"
  },
  "directories": {
    "test": "test"
  },
  "homepage": "https://github.com/dionmaicon/vue-crudgen#readme",
  "keywords": [
    "CRUD",
    "Vue.js",
    "Generator",
    "Boostrap",
    "Vuetify"
  ],
  "license": "MIT",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dionmaicon/vue-crudgen.git"
  }
}
